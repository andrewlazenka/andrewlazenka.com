{
  "name": "andrewlazenka.com",
  "description": "Welcome to my little corner of the metaverse!",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css",
    "build:svg": "npx @svgr/cli --out-dir ./app/components/svg ./app/assets/svg",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "prettier": "prettier --write .",
    "postinstall": "remix setup node",
    "start": "npm run dev"
  },
  "dependencies": {
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@remix-run/vercel": "^1.1.1",
    "@supabase/supabase-js": "^1.29.1",
    "@tiptap/extension-document": "^2.0.0-beta.15",
    "@tiptap/extension-heading": "^2.0.0-beta.25",
    "@tiptap/extension-paragraph": "^2.0.0-beta.23",
    "@tiptap/extension-text": "^2.0.0-beta.15",
    "@tiptap/react": "^2.0.0-beta.105",
    "@tiptap/starter-kit": "^2.0.0-beta.170",
    "classnames": "^2.3.1",
    "concurrently": "^6.4.0",
    "date-fns": "^2.27.0",
    "dotenv": "^10.0.0",
    "front-matter": "^4.0.2",
    "history": "^5.2.0",
    "lodash": "^4.17.21",
    "marked": "^4.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intersection-observer": "^8.32.5",
    "react-spring": "^9.4.2",
    "remix": "^1.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@svgr/cli": "^6.1.2",
    "@tailwindcss/typography": "^0.5.0",
    "@types/classnames": "^2.3.1",
    "@types/lodash": "^4.14.178",
    "@types/marked": "^4.0.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.4",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.3",
    "tailwindcss": "^3.0.13",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
